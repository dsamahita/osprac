#include <iostream>
using namespace std;

int main() {
    int b, p;
    cin >> b;
    int block[b];
    for(int i=0;i<b;i++) cin >> block[i];

    cin >> p;
    int process[p];
    for(int i=0;i<p;i++) cin >> process[i];

    for(int i=0;i<p;i++){
        for(int j=0;j<b;j++){
            if(block[j] >= process[i]){
                cout << "Process " << process[i] << " -> Block " << j+1 << endl;
                block[j] -= process[i];
                break;
            }
        }
    }
    return 0;
}
