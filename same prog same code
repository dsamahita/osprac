#include <iostream>
#include <unistd.h>
using namespace std;

int main() {
    pid_t pid = fork();

    if (pid < 0) {
        cout << "Fork failed\n";
    } 
    else if (pid == 0) {
        cout << "Child Process. PID = " << getpid() << endl;
    } 
    else {
        cout << "Parent Process. PID = " << getpid() << endl;
    }
    return 0;
}
