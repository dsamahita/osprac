#include <iostream>
#include <pthread.h>
using namespace std;

long long sum = 0;

void* compute(void* arg) {
    int* arr = (int*) arg;
    for(int i = 1; i <= arr[0]; i++)
        sum += arr[i];
    return NULL;
}

int main() {
    int n;
    cin >> n;

    int arr[n+1];
    arr[0] = n;

    for(int i=1;i<=n;i++)
        cin >> arr[i];

    pthread_t t1;
    pthread_create(&t1, NULL, compute, (void*)arr);
    pthread_join(t1, NULL);

    cout << "Sum = " << sum << endl;
    return 0;
}
