#include <iostream>
using namespace std;

int main() {
    int b, p;
    cin >> b;
    int block[b];
    for(int i=0;i<b;i++) cin >> block[i];

    cin >> p;
    int process[p];
    for(int i=0;i<p;i++) cin >> process[i];

    for(int i=0;i<p;i++){
        int worst = -1;
        for(int j=0;j<b;j++){
            if(block[j] >= process[i]) {
                if(worst == -1 || block[j] > block[worst])
                    worst = j;
            }
        }
        if(worst != -1){
            cout << "Process " << process[i] << " -> Block " << worst+1 << endl;
            block[worst] -= process[i];
        }
    }
    return 0;
}
